#include <iostream>
#include <windows.h>

int main(void)
{
	HWND stealth;
	AllocConsole();
	stealth = FindWindowA("ConsoleWindowClass", NULL);
	ShowWindow(stealth, 0);
	{
		Sleep(300);
		{
			Beep(300, 500);
			
			Beep(300, 500);
			
			Beep(300, 500);
			

			Beep(250, 500);
			

			Beep(350, 250);
			Beep(300, 500);
			

			Beep(250, 500);
			

			Beep(350, 250);
			Beep(300, 500);
			

			getchar();
			return 0;
		}
	}
}
